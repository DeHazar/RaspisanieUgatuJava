<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="hazarsoftware.raspisanieugatu">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application

        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="GoogleAppIndexingWarning">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".DayOfScheduleList"
            android:label="@string/app_name">
        </activity>
        <activity android:name=".ContentSetingActivity">
        </activity>
        <activity android:name=".NotesPart.addnote.AddNoteActivity"
            tools:ignore="InnerclassSeparator">
        </activity>
        <activity android:name=".NotesPart.notes.NotesActivity"
            tools:ignore="InnerclassSeparator">
        </activity>
        <activity android:name=".NotesPart.notedetail.NoteDetailActivity"
            tools:ignore="InnerclassSeparator">
        </activity>
    </application>

</manifest>